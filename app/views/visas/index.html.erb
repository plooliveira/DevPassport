<h1>Visas and Tests</h1>

<% @visas.each do |visa| %>
  <h2>
    <%= visa.title %>
  </h2>
  <h3>
    <%= visa.description %>
  </h3>
  <% visa.tests.each do |test| %>
    <ul>
      <li>Author: test.user.name (corrigir ap√≥s migration de users)</li>
      <li>Title: <%= test.title %></li>
      <li>Description: <%= test.description %></li>
      <li>Seniority: <%= test.seniority %></li>
    </ul>
  <% end %>
<% end %>
